



RewriteEngine on
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.html -f
RewriteRule ^(.*)$ $1.html [NC,L]



RewriteEngine On

RewriteRule ^(.*)$ https://phrasos.com [NC]

RewriteCond %{HTTP_HOST} ^phrasos.com/$1 [L,R=301]


RewriteEngine On
RewriteCond %{HTTP_HOST} ^www.phrasos.com [NC]
RewriteRule ^(.*)$ https://phrasos.com/$1 [L,R=301]

RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://phrasos.com/$1 [R,L]